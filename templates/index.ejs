<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Ginkgoch - Shapefile</title>

    <link rel="stylesheet" href="./assets/bootstrap.min.css" >
    <link rel="stylesheet" href="./assets/leaflet.css" >
    <link rel="stylesheet" href="./assets/content.css" >
</head>
<body>
    <script>
        let state = `<%- JSON.stringify(overview) %>`;
        state = JSON.parse(state);
    </script>
    <div class="container root">
        <h1 class="title">Shapefile - {{ name }}</h1>
        <div class="row">
            <div class="col-md-6 col-sm-12">
                <div id="mapContainer" class="map-container bg-light"></div>
            </div>
            <div class="col-md-6 col-sm-12">
                <div class="map-info bg-light">
                    <table class="table table-striped">
                        <tbody>
                            <tr v-for="(value, key) in general">
                                <th>{{ key }}</td>
                                <td>{{ value }}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="map-table table-scroll">
                <div>
                    Fetched the top <span class="text-primary">{{ properties.length - 1 }}</span> records of <span class="text-primary">{{ totalCount }}</span class="text-primary">
                </div>
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th v-for="item in properties[0]">{{ item }}</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr v-for="row in properties.slice(1, 10)">
                            <td v-for="col in row">{{ col }}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>

    <script src="./assets/lodash.min.js"></script>
    <script src="./assets/vue.min.js"></script>
    <script src="./assets/leaflet.js"></script>
    <script src="./assets/axios.min.js"></script>
    <script src="./assets/content.js"></script>
</body>
</html>